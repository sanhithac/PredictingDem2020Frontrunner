#hillary clinton
def get_url (q):
    url = ("http://api.nytimes.com/svc/semantic/v2/concept/name/nytd_per/Clinton, Hillary?fields=all&api-key=MRAJUxGvim9gZYw4sGZ0ZH9hAgLrAeDR")
    return url

r = requests.get(get_url("Clinton, Hillary"))

#r.json()

